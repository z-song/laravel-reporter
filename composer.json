{
    "name": "encore/laravel-reporter",
    "description": "laravel exceptions reporter",
    "type": "library",
    "keywords": ["laravel", "exception", "reporter", "dashboard", "platform"],
    "homepage": "https://github.com/z-song/laravel-reporter",
    "license": "MIT",
    "authors": [
        {
            "name": "zsong",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "~5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "laravel/laravel": "~5.3",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Encore\\Reporter\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    }
}
